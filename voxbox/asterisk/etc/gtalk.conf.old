[general]
context=google-in                ;where to place calls
allowguest=yes                 ;allow calls from non peers

#externip=X.X.X.X              ;not too sure some places recommend this setting


[guest]
disallow=all                  ;turn off all codecs   
allow=ulaw                    ;allow ulaw
allow=speex                   ;allow speex
context=google-in             ; where does the call come from

[buddy]
username=buddy@gmail.com      ;your gtalk buddies name
disallow=all 
allow=ulaw 
allow=speex
context=google-in 
connection=gtalk_account